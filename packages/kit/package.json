{
  "name": "@devvit/kit",
  "version": "0.10.20",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://developers.reddit.com/"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo coverage dist",
    "clobber": "yarn clean && rm -rf node_modules",
    "dev": "yarn build -w",
    "lint": "redlint ./src",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "publish-package-json",
    "test": "concurrently npm:test:unit npm:test:types",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit-with-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@devvit/public-api": "0.10.20"
  },
  "devDependencies": {
    "@devvit/eslint-config": "0.10.20",
    "@devvit/repo-tools": "0.10.20",
    "@devvit/tsconfig": "0.10.20",
    "concurrently": "7.5.0",
    "eslint": "8.9.0",
    "typescript": "5.3.2",
    "vitest": "0.31.0"
  },
  "publishConfig": {
    "directory": "dist"
  }
}
